# .env.example (Frontend)
# Passe die Werte an deine Backend-Konfiguration an

# Backend API URL
VITE_API_BASE_URL=http://localhost:5000/api

# App Konfiguration
VITE_APP_TITLE="Chris Schubert - Web Developer"
VITE_CONTACT_EMAIL=schubert_chris@rocketmail.com

# OAuth URLs (für Frontend-Redirects)
VITE_GOOGLE_OAUTH_URL=http://localhost:5000/api/auth/google
VITE_GITHUB_OAUTH_URL=http://localhost:5000/api/auth/github

# Development/Production Umschaltung
VITE_NODE_ENV=development

# Optional: Für spätere Features
VITE_DASHBOARD_URL=http://localhost:5173/dashboard
VITE_SUCCESS_REDIRECT=http://localhost:5173/oauth-success
VITE_ERROR_REDIRECT=http://localhost:5173/oauth-error